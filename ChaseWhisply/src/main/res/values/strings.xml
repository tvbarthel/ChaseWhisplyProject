    <string name="tuto_page_welcome_title">Welcome,</string>
    <string name="tuto_page_welcome_subtitle">padawan.</string>
    <string name="tuto_page_welcome_content">Your mission is to seek, find and kill
        the ghosts living around you.
    </string>
    <string name="tuto_page_welcome_additional_text">Swipe left to learn more.</string>

    <!-- Tuto Fragment Page Play Button -->
    <string name="tuto_page_play_button_content">Select your favorite mode and hunt the ghosts around you.</string>

    <!--Tuto Fragment Page Profile Button -->
    <string name="tuto_page_profile_button_content">Follow your progress and discover information about your enemy.</string>

    <!-- Tuto Fragment Page Leaderboard Button -->
    <string name="tuto_page_leaderboard_button_content">Compare your score with your friends\' and try to be the best
        hunter in the World.
    </string>

    <!-- Tuto Fragment Page Achievement Button -->
    <string name="tuto_page_achievement_button_content">Improve yourself as a ghost hunter and keep track of your
        progress.
    </string>

    <!-- Tuto Fragment Page Aim Tip -->
    <string name="tuto_page_aim_tip_content">In order to target a ghost accurately, try to picture your phone as a
        gunsight. Move your arms, not only your hands.
    </string>

    <!-- Tuto Fragment Page Target -->
    <string name="tuto_page_target_content">Your targets are Whisply and his siblings.</string>
    <string name="tuto_page_target_bis_content">Line up one of them in your gunsight by moving your phone, then shoot by touching the
        screen.
    </string>

    <!-- Tuto Fragment Page Ammo -->
    <string name="tuto_page_ammo_content">Watch out your ammunition on the bottom right of your screen, or you will run
        out.
    </string>
    <string name="tuto_page_ammo_bis_content">Your ammunition increases with time.</string>

